{
    "name": "leocolomb/wp-redis",
    "description": "A persistent object cache backend for WordPress powered by Redis.",
    "type": "wordpress-dropin-bundle",
    "homepage": "https://github.com/LeoColomb/wp-redis",
    "license": "GPL-3.0+",
    "authors": [
        {
            "name": "Till Krüss",
            "homepage": "https://till.im/"
        },
        {
            "name": "Léo Colombaro",
            "homepage": "https://colombaro.fr/"
        }
    ],
    "keywords": [
        "wordpress-dropin",
        "cache-object",
        "redis"
    ],
    "support": {
        "issues": "https://github.com/LeoColomb/wp-redis/issues"
    },
    "autoload": {
        "classmap": [
            "src/"
        ]
    },
    "require": {
        "php": ">=5.6",
        "composer/installers": "^1.0",
        "koodimonni/composer-dropin-installer": "^1.2"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^2.5"
    },
    "scripts": {
        "test": [
            "phpcs"
        ]
    },
    "suggest": {
        "predis/predis": "Using a PHP-Redis interface",
        "ext-redis": "Using a native-Redis interface"
    }
}
