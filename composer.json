{
    "name": "leocolomb/wp-redis-dropin",
    "description": "A persistent object cache backend for WordPress powered by Redis. Supports Predis, PhpRedis, HHVM, replication and clustering.",
    "homepage": "https://github.com/leocolomb/redis-cache",
    "license": "GPL-3.0+",
    "authors": [
        {
            "name": "Till Krüss",
            "homepage": "https://till.im/"
        },
        {
            "name": "Léo Colombaro",
            "homepage": "https://colombaro.fr/"
        }
    ],
    "keywords": [
        "cache-object",
        "redis"
    ],
    "support": {
        "issues": "https://github.com/leocolomb/redis-cache/issues"
    },
    "autoload": {
        "classmap": [
            "src/"
        ]
    },
    "require": {
        "php": ">=5.4.0",
        "composer/installers": "^1.0",
        "koodimonni/composer-dropin-installer": "*",
    },
    "suggest": {
        "predis/predis": "Using a PHP-Redis interface",
        "ext-redis": "Using a native-Redis interface"
    }
}
