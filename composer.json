{
    "name": "leocolomb/wp-redis",
    "description": "A persistent cache backend for WordPress powered by Redis.",
    "keywords": [
        "wordpress-dropin",
        "wordpress-cache",
        "cache-object",
        "cache-page",
        "cache-advanced",
        "redis",
        "wordpress"
    ],
    "homepage": "https://github.com/LeoColomb/wp-redis",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Till Krüss",
            "homepage": "https://till.im/"
        },
        {
            "name": "Léo Colombaro",
            "homepage": "https://colombaro.fr/"
        }
    ],
    "require": {
        "php": ">=7.1",
        "koodimonni/composer-dropin-installer": "^1.2"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.3"
    },
    "suggest": {
        "ext-redis": "Using a native-Redis interface",
        "predis/predis": "Using a PHP-Redis interface"
    },
    "autoload": {
        "classmap": [
            "src/"
        ]
    },
    "scripts": {
        "lint": [
            "phpcs"
        ]
    },
    "support": {
        "issues": "https://github.com/LeoColomb/wp-redis/issues"
    }
}
